{
  "name": "client-side",
  "version": "1.0.0",
  "description": "App for library project",
  "main": "server.js",
  "scripts": {
    "test": "test",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --bail",
    "buildTsc": "tsc -p src/",
    "buildTsc:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p src/ -w",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run buildTsc:watch\" \"npm run serve\"",
    "pre-protractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.json",
    "pretest": "npm run buildTsc",
    "webpack-server": "webpack-dev-server --inline --progress --port 8080"
  },
  "author": "Chris Meehan",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "@types/node": "^7.0.14",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "null-loader": "^0.1.1",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "tslib": "^1.6.1",
    "typescript": "^2.2.2",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.9"
  },
  "repository": {},
  "devDependencies": {
    "webpack-server": "^0.1.2"
  }
}
